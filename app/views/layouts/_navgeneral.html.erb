<div class="container">     
<div class="navbar">
      <!-- Everything you want hidden at 940px or less, place within here -->
      <div class="nav-collapse collapse">
          <% if user_signed_in? %>
          <ul class="nav-white"> 
          <li style="float:left; margin-top:12px"><%= link_to image_tag("logo.png", :alt => "Uvalue Logo", :size => "200x120"), root_path %></li>
        <li class="masthead"><%= link_to "View My Profile", user_path(current_user) %></li>
    <li class="masthead"><%= link_to "Go to My Admin Dashboard", root_path %></li>
    <li class="masthead" style="margin-top: 20px;">  Signed in as <strong><%= current_user.name %></strong></li>
      <li class="masthead"> <%= link_to "Sign Out", destroy_user_session_path, method: :delete %></li>
    </ul>
          <% else %>
          <ul class="nav-white"> 
          <li style="float:left; margin-top:5px"><%= link_to image_tag("logo.png", :alt => "Uvalue Logo", :size => "200x120"), root_path %></li>
        <li class="masthead"><%= link_to "Join Our Marketplace", root_path %></li>
    <li class="masthead"><%= link_to "Sell Your Items Online", root_path %></li>
    <li class="masthead"><%= link_to "Pricing", root_path %></li>
    <span class="masthead"><%= link_to "User Login", new_user_registration_path, class: "btn btn-small btn-success" %>
        <%= link_to "Business Login", new_merchant_session_path, class: "btn btn-small btn-inverse" %></span>
    <% end %>
        
    <!--<li><%= link_to "Sign Up", '#' %></li>-->
    </ul>
<!-- .nav, .navbar-search, .navbar-form, etc -->
      </div>

    </div> 


